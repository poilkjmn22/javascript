{
  "presets": [
    ["@babel/preset-react"],
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        },
        "useBuiltIns": "usage",
        "modules": "commonjs",
        "corejs": {
          "version": "3.9",
          "proposal": true
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-syntax-dynamic-import"]
}
