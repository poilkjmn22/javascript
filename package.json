{
  "name": "javascript",
  "version": "1.0.0",
  "description": "base knowledge",
  "main": "base.js",
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js ./utils",
    "start": "concurrently \"npm run dev-serve\" \"npm run serve\"",
    "dev-serve": "webpack serve --config webpack.dev.config.js",
    "serve": "npm run build && node ./build/server.js",
    "server": "node ./build/server.js",
    "dump": "cp /usr/local/var/db/redis/dump.rdb dump;cp ~/.vimrc ../atom-config/backup",
    "restore": "cp dump/dump.rdb /usr/local/var/db/redis;cp ../atom-config/backup/.vimrc ~/vimfiles",
    "build": "npm run build-server && npm run build-client",
    "build-server": "webpack --config webpack.server.config.js",
    "build-client": "webpack --config webpack.prod.config.js",
    "profile-prod": "webpack --config webpack.prod.config.js --profile --json > analysis/stats-prod.json",
    "profile-dev": "webpack --config webpack.dev.config.js --profile --json > analysis/stats-dev.json",
    "analyze-bundle-dev": "webpack serve --config webpack.analysis-dev.config.js",
    "analyze-bundle-prod": "webpack --config webpack.analysis-prod.config.js"
  },
  "author": "fq",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "@fangqi/web-dev-cli": "^1.0.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.1.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.1.1",
    "eslint": "^7.18.0",
    "eslint-webpack-plugin": "^2.4.3",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.2.15",
    "postcss-css-variables": "^0.18.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-mixins": "^8.0.0",
    "postcss-nested": "^5.0.5",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.18.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.9.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinyin": "^2.10.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-common-components-fq": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "redis": "^3.1.2",
    "sass-common-mixins-fq": "^1.0.0",
    "vue": "^2.6.12",
    "wangeditor": "^4.7.5"
  }
}
