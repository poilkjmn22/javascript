{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-undef": "error",
      "no-unused-vars": "warn",
      "no-prototype-builtins": "off"
    },
    "globals": {
      "console": "readable",
      "Symbol": "readable",
      "Map": "readable",
      "Set": "readable",
      "setTimeout": "readable",
      "Proxy": "readable",
      "Promise": "readable",
      "XMLHttpRequest": "readable",
      "fetch": "readable",
      "Headers": "readable"
    }
}
