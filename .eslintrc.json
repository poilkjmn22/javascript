{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-undef": "error",
      "no-unused-vars": "warn"
    },
    "globals": {
      "console": "readable",
      "Symbol": "readable",
      "Map": "readable",
      "Set": "readable"
    }
}
